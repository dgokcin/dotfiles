# Lists everything in a directory
alias la="ls -a"
# Go back 1 directory level (for fast typers)
alias cd..='cd ../'
alias ..='cd ../' 
alias ....='cd ../../' 
alias ......='cd ../../../' 
alias ........='cd ../../../../' 
# Quickly search for file
alias qfind='find . -name'
alias qsearch='grep -rnw . -e'
# Quickly edit vimrc and bash_profile
alias vimrc='vim ~/.vim_runtime/my_configs.vim'
alias bashrc='vim ~/.bash_profile'
alias creds='vim ~/.aws/credentials'
#alias rm='rm -i'

alias pip=pip3
alias python=python3

