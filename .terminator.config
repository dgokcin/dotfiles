global_config]
  sticky = True
  title_hide_sizetext = True
  title_transmit_bg_color = "#f92672"
  title_inactive_bg_color = "#75715e"
  enabled_plugins = APTURLHandler, LaunchpadCodeURLHandler, LaunchpadBugURLHandler
  suppress_multiple_term_dialog = True
[keybindings]
  split_horiz = <Primary><Shift>o
  split_vert = <Primary>e
[profiles]
  [[default]]
    icon_bell = False
    background_color = "#0d0d0d"
    cursor_color = "#ffffff"
    font = Ubuntu Mono 12
    foreground_color = "#ffffff"
    show_titlebar = False
    use_system_font = False
[layouts]
  [[default]]
    [[[child1]]]
      type = Terminal
      parent = window0
      profile = monokai
    [[[window0]]]
      type = Window
      parent = ""
      size = 1623, 876
[plugins]
